{
    "name": "listtodo",
    "version": "1.0.0",
    "description": "Practice webpack and react",
    "main": "index.js",
    "scripts": {
        "webpack-server-dev": "webpack-dev-server",
        "prod": "rm -rf ./dist && webpack --env.mode production",
        "dev": "npm run webpack-server-dev -- --env.mode development",
        "build": "rm -rf ./dist && parcel build --public-url ./dist src/App/clientApp/index.html",
        "start": "npm run build && babel-node ./src/server/index.js",
        "server-side-rendering": "node ./dist src/index.html",
        "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\"",
        "lint": "eslint ./",
        "lint:fix": "eslint ./ --fix",
        "server-side": "webpack --prograss && node server"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Yelison/ListTodo.git"
    },
    "author": "Yelisson Ortiz",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Yelison/ListTodo/issues"
    },
    "homepage": "https://github.com/Yelison/ListTodo#readme",
    "dependencies": {},
    "devDependencies": {
        "@babel/core": "^7.7.2",
        "@babel/node": "^7.7.0",
        "@babel/preset-env": "^7.7.1",
        "@babel/preset-react": "^7.7.0",
        "@reach/router": "^1.2.1",
        "babel-core": "^6.26.3",
        "babel-loader": "^8.0.6",
        "babel-node": "0.0.1-security",
        "babel-preset-es2015": "^6.24.1",
        "css-loader": "^3.2.0",
        "es2015": "0.0.0",
        "eslint": "^6.7.1",
        "eslint-plugin-react": "^7.16.0",
        "export-files": "^2.1.1",
        "express": "^4.17.1",
        "extract-css-chunks-webpack-plugin": "^4.7.0",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^5.0.2",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^5.5.0",
        "image-webpack-loader": "^6.0.0",
        "img-loader": "^3.0.1",
        "js-loader": "^0.1.1",
        "lazy": "^1.0.11",
        "mini-css-extract-plugin": "^0.8.0",
        "node-sass": "^4.13.0",
        "normalize.css": "^8.0.1",
        "parcel": "^1.12.4",
        "prettier": "^1.19.1",
        "prop-types": "^15.7.2",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-router-dom": "^5.1.2",
        "sass": "^1.23.7",
        "sass-loader": "^8.0.0",
        "style-loader": "^1.0.0",
        "url-loader": "^3.0.0",
        "webpack": "^4.41.2",
        "webpack-cli": "^3.3.12",
        "webpack-combine-loaders": "^2.0.4",
        "webpack-dev-middleware": "^3.7.2",
        "webpack-dev-server": "^3.9.0",
        "webpack-merge": "^4.2.2",
        "webpack-node-externals": "^1.7.2"
    }
}
